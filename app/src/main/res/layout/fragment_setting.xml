<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".Setting">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_margin="20dp"
        android:orientation="vertical"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:id="@+id/editprofile"
            android:text="Edit Profile"
            android:background="@drawable/faq_edit_button"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:id="@+id/setting_notification"
            android:text="Notifictions"
            android:background="@drawable/faq_edit_button"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:id="@+id/booked_and_history"
            android:text="Booked and visit history"
            android:background="@drawable/faq_edit_button"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:text="Prescriptions and Assessments"
            android:background="@drawable/faq_edit_button"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:text="Payment history"
            android:background="@drawable/faq_edit_button"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:text="Add Patient"
            android:id="@+id/add_patient"
            android:background="@drawable/faq_edit_button"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:id="@+id/logout"
            android:text="Logout"
            android:background="@drawable/faq_edit_button"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</LinearLayout>